{
  "manifest_version": 3,
  "name": "EDBB Themes Plus",
  "version": "3.1",
  "description": "テーマを追加する拡張機能です",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://himais0giiiin.com/beta*",
        "https://himais0giiiin.com/editor/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "themes.css"
      ],
      "run_at": "document_end"
    }
  ]
}